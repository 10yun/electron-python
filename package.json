{
  "name": "cvjs-electron-python",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "http://cvjs.cn/",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL1": "http://cvjs.cn/",
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build-mac": "vite build --mode production && electron-builder --mac",
    "build-win": "vite build --mode production && electron-builder --win",
    "build2": "vue-tsc --noEmit && vite build && electron-builder build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "child_process": "^1.0.2",
    "element-plus": "^2.6.2",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "29.1.5",
    "electron-builder": "^24.13.3",
    "vite": "^5.2.6",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.7"
  }
}
